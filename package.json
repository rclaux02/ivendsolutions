{
  "name": "vape-vending-machine",
  "version": "1.0.0",
  "main": "./dist/main/main.js",
  "scripts": {
    "start": "npm run build && cross-env NODE_ENV=production electron .",
    "start:quiet": "npm run build && cross-env NODE_ENV=production ELECTRON_ENABLE_LOGGING=0 electron .",
    "dev": "vite",
    "dev:quiet": "cross-env VITE_SILENT=true vite",
    "electron-dev": "concurrently \"cross-env NODE_ENV=development vite\" \"wait-on http://localhost:5173 && tsc -p tsconfig.main.json && cross-env NODE_ENV=development electron .\"",
    "electron-dev:quiet": "concurrently \"cross-env NODE_ENV=development VITE_SILENT=true vite\" \"wait-on http://localhost:5173 && tsc -p tsconfig.main.json && cross-env NODE_ENV=development ELECTRON_ENABLE_LOGGING=0 electron .\"",
    "build": "tsc -p tsconfig.main.json && vite build",
    "watch": "tsc -w -p tsconfig.main.json",
    "lint": "eslint . --ext .ts,.tsx",
    "test": "jest",
    "test:rappi-inventory": "npx ts-node scripts/test-rappi-inventory-simple.ts",
    "pack": "npm run build && electron-builder --dir",
    "dist": "npm run build && electron-builder",
    "postinstall": "electron-builder install-app-deps"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@types/express": "^5.0.1",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.17.43",
    "@types/node-webcam": "^0.8.3",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/serialport": "^8.0.2",
    "@vitejs/plugin-react": "^4.2.0",
    "autoprefixer": "^10.4.20",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "electron": "^28.1.0",
    "electron-builder": "^24.9.1",
    "electron-reloader": "^1.2.3",
    "jest": "^29.7.0",
    "postcss": "^8.5.3",
    "tailwindcss": "^3.4.17",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "vite": "^5.0.0",
    "wait-on": "^7.2.0"
  },
  "dependencies": {
    "@regulaforensics/document-reader-webclient": "^7.5.206",
    "@regulaforensics/facesdk-webclient": "^5.1.0",
    "@tensorflow/tfjs": "^4.22.0",
    "@tensorflow/tfjs-backend-wasm": "^4.22.0",
    "@tensorflow/tfjs-backend-webgl": "^4.22.0",
    "@tensorflow/tfjs-core": "^4.22.0",
    "@types/xml2js": "^0.4.14",
    "@vladmandic/human": "^3.3.5",
    "axios": "^1.6.5",
    "basic-ftp": "^5.0.3",
    "canvas": "^3.1.0",
    "dotenv": "^16.5.0",
    "electron-store": "^8.1.0",
    "express": "^4.21.2",
    "lucide-react": "^0.475.0",
    "mysql2": "^3.6.1",
    "node-webcam": "^0.8.2",
    "react": "^18.2.0",
    "react-circular-progressbar": "^2.1.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.21.1",
    "serialport": "^10.5.0",
    "tesseract.js": "^4.1.1",
    "url": "^0.11.4",
    "xml2js": "^0.6.2"
  },
  "build": {
    "appId": "com.vape.vendingmachine",
    "productName": "Vape Vending Machine",
    "icon": "build/vapeBoxSquareLogo.ico",
    "directories": {
      "output": "release"
    },
    "files": [
      "dist/**/*",
      "package.json",
      {
        "from": "public/assets",
        "to": "assets"
      },
      ".env"
    ],
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64"
          ]
        }
      ]
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "createDesktopShortcut": true,
      "artifactName": "${productName}-Setup.${ext}"
    },
    "extraResources": [
      {
        "from": "node_modules/@u4/opencv4nodejs/build/Release/",
        "to": "opencv4nodejs",
        "filter": [
          "**/*"
        ]
      },
      {
        "from": "public/scripts/RegulaScripts",
        "to": "scripts/RegulaScripts",
        "filter": [
          "**/*"
        ]
      }
    ],
    "asar": false
  },
  "opencv4nodejs": {
    "disableAutoBuild": true,
    "opencvIncludeDir": "C:\\Users\\stefa\\Documents\\Projects\\Vending Machine\\Code\\vape-vending-machine\\node_modules\\opencv4nodejs-prebuilt-install\\opencv\\build\\include",
    "opencvLibDir": "C:\\Users\\stefa\\Documents\\Projects\\Vending Machine\\Code\\vape-vending-machine\\node_modules\\opencv4nodejs-prebuilt-install\\opencv\\build\\lib",
    "opencvBinDir": "C:\\Users\\stefa\\Documents\\Projects\\Vending Machine\\Code\\vape-vending-machine\\node_modules\\opencv4nodejs-prebuilt-install\\opencv\\build\\bin"
  }
}
